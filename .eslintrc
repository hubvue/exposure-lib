{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "plugins": ["@typescript-eslint"],
  "extends": ["plugin:prettier/recommended"],
  "rules": {
    "quotes": ["error", "single"]
  }
}
